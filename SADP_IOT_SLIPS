SLIP1 : 
Q2.

#include "WiFi.h"   

void setup() {
Serial.begin(115200);
WiFi.mode(WIFI_STA);
WiFi.disconnect();  
delay(1000);
Serial.println("Scanning networks...");
}

void loop() {
int n = WiFi.scanNetworks();
Serial.println("Scan complete.");
if (n == 0) {
Serial.println("No networks found.");
} 
else 
{
Serial.println("Networks found:");
for (int i = 0; i < n; ++i) {
Serial.print(i + 1);
Serial.print(": ");
Serial.print(WiFi.SSID(i));
Serial.print("  Signal: ");
Serial.print(WiFi.RSSI(i));
Serial.println(" dBm");
}
 }


delay(5000);
}


