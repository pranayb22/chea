const int tempPin = A0;
float temperature;

void setup() {
Serial.begin(9600);
}

void loop() {
int sensorValue = analogRead(tempPin);
float voltage = sensorValue * (5.0 / 1023.0);
temperature = voltage * 100;

Serial.print("Temperature: ");
Serial.print(temperature);
Serial.println(" Â°C");
if (temperature > 30) {  
Serial.println("Alert! High Temperature detected!");
}

delay(1000);  
}

